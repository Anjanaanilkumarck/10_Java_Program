class Box3
{ 
    double height; 
    double weight;
    double width; 
    double volume() 
    { 
        return height * weight * width; 
        
    }
    
} 
public class Boxdemo3
{ 
    public static void main(String[] args){
        Box3 mybox1 = new Box3();
        Box3 mybox2 = new Box3();
        double vol; 
        mybox1.height = 6; 
        mybox1.weight = 8; 
        mybox1.width = 9;
        vol = mybox1.volume();
        System.out.println("Volume is : "+ vol);
        mybox2.height = 9; 
        mybox2.weight = 12; 
        mybox2.width = 60; 
        vol = mybox2.volume();
        System.out.println("Volume is : "+ vol);
        }
    
}
